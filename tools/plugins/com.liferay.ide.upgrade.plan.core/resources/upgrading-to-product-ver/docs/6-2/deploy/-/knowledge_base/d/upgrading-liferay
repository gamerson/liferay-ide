                    <liferay-util:html-top> 
                     <link href="https://portal.liferay.dev:443/docs/6-2/deploy?p_p_id=com_liferay_knowledge_base_web_portlet_DisplayPortlet&amp;p_p_lifecycle=2&amp;p_p_state=normal&amp;p_p_mode=view&amp;p_p_resource_id=kbArticleRSS&amp;p_p_cacheability=cacheLevelPage&amp;_com_liferay_knowledge_base_web_portlet_DisplayPortlet_resourceClassNameId=113674871&amp;_com_liferay_knowledge_base_web_portlet_DisplayPortlet_resourcePrimKey=113846104&amp;_com_liferay_knowledge_base_web_portlet_DisplayPortlet_kbFolderUrlTitle=d&amp;_com_liferay_knowledge_base_web_portlet_DisplayPortlet_urlTitle=upgrading-liferay" rel="alternate" title="RSS" type="application/rss+xml"> 
